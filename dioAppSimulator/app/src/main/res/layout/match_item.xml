<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardViewprincipal"
        android:layout_width="0dp"
        android:layout_height="200dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="88dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,UnknownId" >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/camada"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="22dp"
            tools:layout_editor_absoluteY="12dp">

            <TextView
                android:id="@+id/x"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/x"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                app:layout_constraintBottom_toBottomOf="@+id/tvAwayTimeScrore"
                app:layout_constraintEnd_toStartOf="@+id/tvAwayTimeScrore"
                app:layout_constraintStart_toEndOf="@+id/tvAwayTimeScrore2"
                app:layout_constraintTop_toTopOf="@+id/tvAwayTimeScrore2" />

            <TextView
                android:id="@+id/tvAwayTimeScrore2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/_99"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                tools:layout_editor_absoluteX="157dp"
                tools:layout_editor_absoluteY="57dp" />

            <TextView
                android:id="@+id/tvAwayTimeScrore"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/_99"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                tools:layout_editor_absoluteX="205dp"
                tools:layout_editor_absoluteY="57dp" />

            <ImageView
                android:id="@+id/ivTimeHome"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="16dp"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageView
                android:id="@+id/ivTimeContra"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:layout_editor_absoluteX="247dp"
                tools:layout_editor_absoluteY="18dp"
                tools:srcCompat="@tools:sample/avatars" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                app:layout_constraintEnd_toEndOf="@+id/ivTimeHome"
                app:layout_constraintStart_toStartOf="@+id/ivTimeHome"
                tools:layout_editor_absoluteY="162dp" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                app:layout_constraintEnd_toEndOf="@+id/ivTimeContra"
                app:layout_constraintStart_toStartOf="@+id/ivTimeContra"
                tools:layout_editor_absoluteY="159dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>